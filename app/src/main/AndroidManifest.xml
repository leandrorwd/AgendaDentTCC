<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tcc.agendadent">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".gui.LoginGui">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".gui.CadastroGui" />
        <activity android:name=".gui.RecuperacaoSenha" />
        <activity android:name=".gui.dentista.AgendaDiaria" />
        <activity android:name=".gui.paciente.MenuPaciente" />
        <activity android:name=".gui.dentista.ConfigAgenda" />
        <activity android:name=".gui.dentista.ConfigAgendaAdiciona"></activity>
    </application>

</manifest>